    <string name="str_achieve_1_title">Probation Period I</string>
    <string name="str_achieve_1_description">Get 100 Exp to become accepted.</string>
    
    <string name="str_achieve_2_title">Probation Period II</string>
    <string name="str_achieve_2_description">Find 200 devices to know some mac addresses.</string>

    <string name="str_achieve_3_title">Apple Fanboy I</string>
    <string name="str_achieve_3_description">Find 25 Apple devices.</string>
    
    <string name="str_achieve_4_title">Time Chaser I</string>
    <string name="str_achieve_4_description">Find 15 devices within 2 minutes.</string>
    
    <string name="str_achieve_5_title">Superior Supporter</string>
    <string name="str_achieve_5_description">This achievement is for people who are grateful for my work I did on this project. Catch the supporter version of this game on the Google Play Store to accomplish this achievement. I really am thankful for each purchase, because I need your help to keep this game available, to pay the bills for the server and maybe to get some earn. Thank you very much! Ps: Look at the bonus. ;-p</string>
    
    <string name="str_achieve_6_title">Apple Fanboy II</string>
    <string name="str_achieve_6_description">Find 5 Apple devices in a row.</string>
    
    <string name="str_achieve_7_title">Oldies I</string>
    <string name="str_achieve_7_description">Find 5 Siemens devices.</string>
    
    <string name="str_achieve_8_title">Time Geek I</string>
    <string name="str_achieve_8_description">Find 15 devices between 10:00pm and 10:30pm.</string>
    
    <string name="str_achieve_9_title">Time Geek II</string>
    <string name="str_achieve_9_description">Get up early and find 65 devices between 5:00am and 7:00am.</string>
    
    <string name="str_achieve_10_title">Time Chaser II</string>
    <string name="str_achieve_10_description">Do a power hunt and find 100 devices in 15 minutes.</string>
 
    <string name="str_achieve_11_title">Precisely Accurate I</string>
    <string name="str_achieve_11_description">You are an accurate chaser and must find 1 device to a full hour. (E.g. 2:00pm)</string>
    
    <string name="str_achieve_12_title">Sequence Enthusiast I</string>
    <string name="str_achieve_12_description">Find 1 Apple, 1 BlackBerry and 1 Samsung device in a row.</string>
    
    <string name="str_achieve_13_title">Experienced Player I</string>
    <string name="str_achieve_13_description">Get 200 Exp within 24 hours.</string>
    
    <string name="str_achieve_14_title">Precisely Accurate II</string>
    <string name="str_achieve_14_description">You are an extremely accurate chaser and must find 1 device at the 11th November at 11:11am.</string>
    
    <string name="str_achieve_15_title">Probation Period III</string>
    <string name="str_achieve_15_description">Find 300 devices and get 8000 Exp. Now you are out of probation period. Congrats!</string>
    
    <string name="str_achieve_16_title">Time Chaser III</string>
    <string name="str_achieve_16_description">Get on a crowded place and find 20 devices within 15 seconds.</string>
    
    <string name="str_achieve_17_title">Oldies II</string>
    <string name="str_achieve_17_description">Find 50 Sony Ericsson devices.</string>
    
    <string name="str_achieve_18_title">Sequence Enthusiast II</string>
    <string name="str_achieve_18_description">You are an alphabet freak. Find these devices in a row:\nAcer -> Bury -> Cisco -> D-Link</string>
    
    <string name="str_achieve_19_title">Sociality I</string>
    <string name="str_achieve_19_description">Find the device from another player of BlueHunter.</string>
    
    <string name="str_achieve_20_title">Debug Helper</string>
    <string name="str_achieve_20_description">You are seeing this Achievement, because you have helped me alot with debugging. Thank you for that. :-)</string>
    
    <string name="str_achieve_21_title">Palindrome Expert</string>
    <string name="str_achieve_21_description">Find one device at a time, which consists of a palindrome. E.g.: 03:30 or 15:51. Note, 24 hours format is used here!</string>
    
